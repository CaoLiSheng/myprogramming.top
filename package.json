{
  "name": "yxblog",
  "version": "2.2.0",
  "description": "YX's BLOG SITE",
  "keywords": [
    "blog",
    "react",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitee.com/yx1991/blog.git"
  },
  "license": "ISC",
  "author": "CaoLS",
  "scripts": {
    "start": "run-p dev:v1 dev:v2 dev:misc",
    "bundle": "yarn util:build -c bundle",
    "dev:gen": "yarn util:build -c gen",
    "dev:misc": "run-p dev:nodemon 'util:http 60000 public/resources'",
    "dev:nodemon": "nodemon --exec 'zsh ./src/scripts/build/gen.zsh devV1 devV2'",
    "dev:v1-tpl": "yarn util:build -c dev-v1",
    "dev:v1": "run-p 'util:build -c dev-v1' 'util:dev-server site.v1.dev' 'util:http 5555 build/v1/posts'",
    "dev:v2": "run-p 'util:build -c dev-v2' 'util:dev-server site.v2.dev' 'util:http 3333 build/v2/posts'",
    "lint": "run-p lint:style lint:es lint:ts",
    "lint:style": "stylelint --custom-syntax stylelint-plugin-stylus/custom-syntax \"./**/*.{css,scss,stylus}\"",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx,.vue --format=pretty ./",
    "lint:ts": "tsc --noEmit",
    "test:markdown-it": "yarn util:build -c markdown-it",
    "util:build": "zsh ./src/scripts/build.zsh",
    "util:dev-server": "func() { webpack-dev-server --silent --config=cfg/webpack/$1.js; }; func",
    "util:format": "zsh ./src/scripts/format.zsh",
    "util:katex": "zsh ./src/scripts/katex.zsh",
    "util:http": "serve -C -l",
    "util:new": "zsh ./src/scripts/newPost.zsh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{.js,.jsx,.ts,.tsx,.vue}": [
      "lint",
      "git add"
    ]
  },
  "dependencies": {
    "@csstools/normalize.css": "^11.0.1",
    "classnames": "^2.2.6",
    "localforage": "^1.9.0",
    "moment": "^2.27.0",
    "portal-vue": "^2.1.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/classnames": "^2.2.10",
    "@types/fs-extra": "^9.0.1",
    "@types/html2canvas": "^0.0.35",
    "@types/js-yaml": "^3.12.5",
    "@types/katex": "^0.11.0",
    "@types/markdown-it": "^12.0.1",
    "@types/md5": "^2.2.0",
    "@types/node": "^12.12.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "babel-loader": "^8.1.0",
    "babel-preset-typescript-vue": "^1.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": ">=7.0.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "eslint-plugin-vue": "^7.8.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.9",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.3.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babel-vue": "^1.0.6",
