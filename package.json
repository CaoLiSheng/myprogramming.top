{
  "name": "yxblog",
  "version": "1.0.0",
  "description": "YX's BLOG SITE",
  "main": "build/index.js",
  "scripts": {
    "test": "webpack -h",
    "prod": "npm run prod:0 && npm run prod:1",
    "pack:prod": "webpack -p --devtool=false --config=cfg/webpack/gen.prod.js",
    "gen:prod": "node ./build/prod/generator.min.js > log.txt",
    "prod:0": "rm -rf public && webpack -p --devtool=false --config=cfg/webpack/site.prod.js",
    "prod:1": "rm -rf build/prod && npm run pack:prod && npm run gen:prod",
    "pack:dev": "webpack -p --devtool=false --config=cfg/webpack/gen.dev.js",
    "gen:dev": "node ./build/dev/generator.min.js > log.txt",
    "dev:0": "nodemon --exec 'node ./build/dev/generator.min.js > log.txt'",
    "dev:1": "serve -C -l 5555 ./build/posts/",
    "dev:2": "webpack-dev-server -p --devtool=false --config=cfg/webpack/site.dev.js",
    "dev:pre": "rm -rf build/dev && npm run pack:dev && npm run gen:dev",
    "dev": "npm run dev:pre && concurrently --handle-input 'npm run dev:0' 'npm run dev:1' 'npm run dev:2'"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/yx1991/blog.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "CaoLS",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/classnames": "^2.2.10",
    "@types/fs-extra": "^9.0.1",
    "@types/html2canvas": "^0.0.35",
    "@types/md5": "^2.2.0",
    "@types/node": "^12.12.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/showdown": "^1.9.3",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.9",
    "html-webpack-plugin": "^4.3.0",
    "md5": "^2.3.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "react-hot-loader": "^4.12.21",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
