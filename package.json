    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pug": "^2.0.0",
    "pug-plain-loader": "^1.0.0",
    "react-hot-loader": "^4.12.21",
    "sass": "^1.26.10",
